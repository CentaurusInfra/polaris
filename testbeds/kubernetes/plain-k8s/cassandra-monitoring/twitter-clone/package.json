{
  "name": "twitter-clone",
  "version": "1.0.0",
  "description": "A simple REST API for a Twitter-like service that can be used for load testing",
  "main": "main.js",
  "scripts": {
    "start": "npx ts-node ./src/main.ts",
    "build": "npx webpack",
    "docker:build": "npm run build && docker build -t tuwiendsgsloc/tweeter-test:$npm_package_version .",
    "docker:build-latest": "npm run build:docker && docker tag tuwiendsgsloc/tweeter-test:$npm_package_version tuwiendsgsloc/tweeter-test:latest",
    "docker:push-latest": "docker push tuwiendsgsloc/tweeter-test:$npm_package_version && docker push tuwiendsgsloc/tweeter-test:latest",
    "package": "npm run build && tar -czvf ./dist/tweeter-test.tar.gz -C ./dist/build .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "private",
  "private": true,
  "devDependencies": {
    "@types/express": "^4.17.3",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "cassandra-driver": "^4.4.0",
    "express": "^4.17.1"
  }
}
