    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug sloc-k8s-serializer",
            "preLaunchTask": "build-cli-sloc-k8s-serializer",
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/tslib/**"
            ],
            "port": 7777,
            "runtimeExecutable": "npx",
            "runtimeArgs": [
                "nx",
                "serve",
                "cli-sloc-k8s-serializer"
            ],
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug cost-efficiency-slo-controller",
            "preLaunchTask": "build-cost-efficiency-slo-controller",
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/tslib/**"
            ],
            "port": 7777,
            "runtimeExecutable": "npx",
            "runtimeArgs": [
                "nx",
                "serve",
                "slo-cost-efficiency-slo-controller",
            ],
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "sourceMaps": true
        }
    ]
}
