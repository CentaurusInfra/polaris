    "mappings-common-mappings": {
      "root": "libs/mappings/common-mappings",
      "sourceRoot": "libs/mappings/common-mappings/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/mappings/common-mappings/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
