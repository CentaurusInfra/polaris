{
    "$schema": "http://json-schema.org/schema",
    "name": "polaris-nx",
    "version": "0.2.0",
    "generators": {
        "polaris-nx": {
            "factory": "./src/generators/polaris-nx/generator",
            "schema": "./src/generators/polaris-nx/schema.json",
            "description": "polaris-nx generator"
        },
        "slo-mapping-type": {
            "factory": "./src/generators/slo-mapping-type/generator",
            "schema": "./src/generators/slo-mapping-type/schema.json",
            "description": "Create a Polaris SLO Cloud SLO Mapping Type"
        },
        "slo-controller": {
            "factory": "./src/generators/slo-controller/generator",
            "schema": "./src/generators/slo-controller/schema.json",
            "description": "Create an SLO Controller for an existing Polaris SLO Mapping Type"
        }
    }
}
