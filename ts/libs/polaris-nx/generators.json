{
    "$schema": "http://json-schema.org/schema",
    "name": "polaris-nx",
    "version": "0.2.0",
    "generators": {
        "slo-mapping-type": {
            "factory": "./src/generators/slo-mapping-type/generator",
            "schema": "./src/generators/slo-mapping-type/schema.json",
            "description": "Create a Polaris SLO Cloud SLO Mapping Type"
        },
        "slo-controller": {
            "factory": "./src/generators/slo-controller/generator",
            "schema": "./src/generators/slo-controller/schema.json",
            "description": "Create an SLO Controller for an existing Polaris SLO Mapping Type"
        },
        "elasticity-strategy-controller": {
            "factory": "./src/generators/elasticity-strategy-controller/generator",
            "schema": "./src/generators/elasticity-strategy-controller/schema.json",
            "description": "elasticity-strategy-controller generator"
        },
        "elasticity-strategy": {
            "factory": "./src/generators/elasticity-strategy/generator",
            "schema": "./src/generators/elasticity-strategy/schema.json",
            "description": "elasticity-strategy generator"
        }
    }
}
