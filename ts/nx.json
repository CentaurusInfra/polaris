{
  "npmScope": "sloc",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.js": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "sloc-ui": {
      "tags": ["scope:ui"]
    },
    "sloc-ui-e2e": {
      "tags": ["scope:ui"],
      "implicitDependencies": ["sloc-ui"]
    },
    "cpu-usage-slo-controller": {
      "tags": ["scope:slo"]
    },
    "core": {
      "tags": ["scope:core"]
    }
  },
  "affected": {
    "defaultBase": "master"
  }
}
