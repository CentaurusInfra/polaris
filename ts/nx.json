{
  "npmScope": "sloc",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.js": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "projects": {
    "cpu-usage-slo-controller": {
      "tags": [
        "scope:slo"
      ]
    },
    "core": {
      "tags": [
        "scope:core"
      ]
    },
    "kubernetes": {
      "tags": [
        "orchestrator:kubernetes"
      ]
    },
    "ui-sloc-ui": {
      "tags": [
        "scope:ui"
      ]
    },
    "ui-sloc-ui-e2e": {
      "tags": [
        "scope:ui"
      ],
      "implicitDependencies": [
        "ui-sloc-ui"
      ]
    }
  },
  "affected": {
    "defaultBase": "master"
  }
}
