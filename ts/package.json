{
  "name": "sloc",
  "version": "0.0.4",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "docker:build:sloc-ui": "npm run build -- sloc-ui && mv ./dist/apps/sloc-ui ./apps/sloc-ui/dist && docker build -t tuwiendsgsloc/sloc-ui:$npm_package_version -t tuwiendsgsloc/sloc-ui:latest ./apps/sloc-ui",
    "docker:push-latest:sloc-ui": "docker push tuwiendsgsloc/sloc-ui:$npm_package_version && docker push tuwiendsgsloc/sloc-ui:latest",
    "docker:build:slo-cost-efficiency-slo-controller": "docker build -f ./apps/slo/cost-efficiency-slo-controller/Dockerfile --build-arg SLOC_APP_TYPE=slo --build-arg SLOC_APP_NAME=cost-efficiency-slo-controller -t tuwiendsgsloc/slo-cost-efficiency:latest .",
    "docker:build:slo-cpu-usage-slo": "docker build -f ./apps/slo/cpu-usage-slo-controller/Dockerfile --build-arg SLOC_APP_TYPE=slo --build-arg SLOC_APP_NAME=cpu-usage-slo-controller -t tuwiendsgsloc/slo-cpu-usage:latest .",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.3",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "11.2.3",
    "@angular/compiler": "11.2.3",
    "@angular/core": "11.2.3",
    "@angular/forms": "11.2.3",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "11.2.3",
    "@angular/platform-browser-dynamic": "11.2.3",
    "@angular/router": "11.2.3",
    "@grafana/data": "^7.0.4",
    "@kubernetes/client-node": "^0.13.0",
    "@ngx-translate/core": "^13.0.0",
    "class-transformer": "~0.3.1",
    "js-yaml": "^3.14.0",
    "lodash-es": "^4.17.15",
    "prometheus-query": "^2.1.4",
    "reflect-metadata": "~0.1.13",
    "rxjs": "6.5.5",
    "zone.js": "^0.10.2",
    "@ngrx/component-store": "11.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.2",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/template-parser": "1.0.0",
    "@angular/cli": "11.2.2",
    "@angular/compiler-cli": "11.2.3",
    "@angular/language-service": "11.2.3",
    "@nrwl/angular": "11.4.0",
    "@nrwl/cli": "11.4.0",
    "@nrwl/cypress": "11.4.0",
    "@nrwl/eslint-plugin-nx": "11.4.0",
    "@nrwl/jest": "11.4.0",
    "@nrwl/node": "11.4.0",
    "@nrwl/tao": "11.4.0",
    "@nrwl/workspace": "11.4.0",
    "@types/jest": "26.0.8",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "12.12.38",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "6.0.1",
    "cypress": "^4.1.0",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "husky": "^4.3.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.2",
    "lint-staged": "^10.4.0",
    "prettier": "2.2.1",
    "ts-jest": "26.4.0",
    "ts-node": "9.1.1",
    "tslib": "^2.0.1",
    "typescript": "4.0.5"
  }
}
